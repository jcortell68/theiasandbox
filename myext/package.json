{
  "name": "myext",
  "displayName": "myext",
  "description": "whatever",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onView:myext_myView"],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myext.helloWorld",
        "icon": "resources/pen.svg",
        "title": "Hello World"
      },
      {
        "command": "myext.goodbyeWorld",
        "icon": "resources/refresh.svg",
        "title": "Goodbye World"
      }
    ],
    "views": {
      "myext_myViewContainer": [
        {
          "type": "webview",
          "id": "myext_myView",
          "name": "My Webview"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "myext_myViewContainer",
          "title": "My Webview Container",
          "icon": "resources/someicon.svg"
        }
      ]
    }
  },
  "scripts": {
    "clean": "rimraf out && rimraf dist",
    "prepare": "yarn clean && yarn compile-theia && yarn symlink",
    "vscode:prepublish": "yarn run package",
    "compile-theia": "tsc -p . --outDir dist",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "symlink": "symlink-dir . ../plugins/myext"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "ts-loader": "^9.4.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "@vscode/test-electron": "^2.3.0",
    "symlink-dir": "latest"
  }
}
